% filters a list of at files with lowpass as indicated in input arg cutofffunction[filtmat]=filteratfiles_lowpass(filemat, cutoffHz);for index = 1:size(filemat,1);         atgPath = deblank(filemat(index,:));	    [AvgMat,File,Path,FilePath,NTrialAvgVec,StdChanTimeMat,SampRate] = ReadAvgFile(atgPath);         cutoff_ratio = cutoffHz *2/SampRate    	[B, A] = butter(5, cutoff_ratio);     	AvgMat = AvgMat';         filtmat = filtfilt(B,A, AvgMat)';         figure(1)    plot(filtmat(62,:)); hold on, plot(AvgMat(62,pause(1)		%[File,Path,FilePath]=SaveAvgFile([atgPath '.amp'],demodmat,NTrialAvgVec,StdChanTimeMat, SampRate);    %eval(['save ' deblank(atgPath) '.pha phasemat -ascii'])end