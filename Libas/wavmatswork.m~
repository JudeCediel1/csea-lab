% avgmats% average .mat files specified in filematinfunction [outmat] = wavmatswork(filematin)for fileindex = 1 : size(filematin, 1)		strucmat = load(deblank(filematin(fileindex, :)));         mat = strucmat.WaPower;		disp(filematin(fileindex, :))				outmat(:, :, fileindex) = mat(; 		endavgmat = summat ./ size(filematin, 1); if ~isempty(outname)    save(outname, 'avgmat', '-mat')end