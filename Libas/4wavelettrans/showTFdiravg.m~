% showTFdirfunction [] = showTFdiravg(dirname, taxis, faxis);filemat = getfilesindir(dirname)cd(dirname)for index = 1: size(filemat, 1); 	load(filemat(index,:));	pause	contourf(taxis(5:length(taxis)-5), faxis(5:length(faxis)), squeeze(mean(outWaMat(70:90 ,(5:length(faxis)), 5:length(taxis)-5))),15), colorbar	title(filemat(index,:))        flag = input('accept ? [y]   ')    if flag == 'y' or flag == []end