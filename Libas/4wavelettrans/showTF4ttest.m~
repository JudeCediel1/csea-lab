% showTFdirfunction [TFmat3] = showTF4ttest(dirname, filemat, taxis, faxis);if nargin < 5, takevecin = []; else takevec = takevecin; end%filemat = getfilesindir(dirname)cd(dirname)paramat = [];TFmat3 = [];for index = 1: size(filemat, 1); 	load(filemat(index,:), '-mat');	pause(0.1)	contourf(taxis(10:length(taxis)-5), faxis(6:length(faxis)-15), squeeze(mean(avgmat([65:68 70:73 75:78 83:86 90:92] ,(6:length(faxis)-15), 10:length(taxis)-5))),15), colorbar	title(filemat(index,:))        TFmat3(:,:,index) = squeeze(mean(avgmat([65:68 70:73 75:78 83:86 90:92] ,(6:length(faxis)-15), 10:length(taxis)-5)));end