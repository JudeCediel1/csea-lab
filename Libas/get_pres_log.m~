function [condvec] = get_pres_log(filepath_6files)line = 1;index = 1;condvec = [];for fileindex = 1:6fid = fopen(filepath_6files(fileindex,:))% dummylinesdumline = fgetl(fid);dumline = fgetl(fid);dumline = fgetl(fid);dumline = fgetl(fid);dumline = fgetl(fid);%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%  			while line > 0								line = fgets(fid)						% segmentiere jede zeile nach tabs				trialnumseg = find(line == '	');				% event-tags		        if line > 0 % read data				eventnum = line(trialnumseg(1)+1:trialnumseg(2)-1);				eventcateg = line(trialnumseg(2)+1:trialnumseg(3)-1);				eventcode = (line(trialnumseg(3)+1:trialnumseg(4)-1))                if eventcode == 'iti'             condvec = condvec        elseif eventcode == '11HL'            condvec(index) = 1; index = index + 1;        elseif eventcode == '12HL'            condvec(index) = 1; index = index + 1;        elseif eventcode == '13HL'            condvec(index) = 1; index = index + 1;        elseif eventcode == '21HL'            condvec(index) = 1; index = index + 1;        elseif eventcode == '22HL'            condvec(index) = 1; index = index + 1;        elseif eventcode == '23HL'            condvec(index) = 1; index = index + 1;        elseif eventcode == '31HL'            condvec(index) = 1; index = index + 1;        elseif eventcode == '31HL'            condvec(index) = 1; index = index + 1;            		        end				% for correctflag    endend	