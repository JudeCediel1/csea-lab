function [namelist, paths, names] = wr_mn_meg(sensor_name, cos_name, paths, names);if nargin==0,   disp(' [namelist, paths, names] = wr_mn_meg(sensor_name, cos_name, paths, names); ');   return;end;if isempty(paths),   [namelist, names, paths] = read_filelist('*.avr');end;% Read output locationslocations = read_matrix(87, 3, '/home/assadoll/Programme/MN/elp87xyz.dat')';% lfd_out_name = sprintf('%s%s.lfd', deblank(paths(1,:)), deblank(names(1,1:3)));lfdmat = '';% from = startzeit kann auch negativ sein, to = endzeitfrom= -100;to= 498;counter = 1;%letzter parameter (200) range: 0-unendlich% bestimmt, wie gut der fit wird   lfdmat = mn_MEG_batch(sensor_name, cos_name, paths, names, lfdmat, '/export/jesse/data3/SMMN', '', '', '', 200);%   lfd_out_name = sprintf('%s%s.lfd', deblank(paths(i,:)), deblank(names(i,1:3)));%   fid_lfd = fopen(lfd_out_name, 'w');%   fwrite(fid_lfd, lfdmat, 'float');%   fclose(fid_lfd);   lfdmat = '';%deblank(paths(i,:))