% GENERDATNEWfunction[data]=gd_new();t = [0:0.00:pi]; sig1 = sin(2*pi*t*12.1).*5 + (rand(size(t))-0.5).*4.9;sig2 = sin(2*pi*2*t).*6;s1 = (rand(1,745)-0.5).*.5;s2 = (rand(1,345)-0.5).*.5 ;sigAdd = [zeros(1,300) sin(4*pi*t(1:90)).*7 zeros(1,355) ];data1 = [ s1+sigAdd sig1 s2];%data2 =[s2 sig2 s1];%data = data1 + data2;data = data1;%datanoise = rand(1, 2048).*5;plot(data, 'k')