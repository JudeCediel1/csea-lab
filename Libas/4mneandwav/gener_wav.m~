% generates morlet wavelets with given frequency range etc.function [wavelet] = gener_Wav(NPoints, delta_f0, f0_start, f0_end);plotyon= 'n';m=7;index = 1;for f0 = f0_start : delta_f0 : f0_end    %   ueber  Peakfrequenz der Wavelets : index f0 bis nyquist		sigma_f = f0 / m;		A = 1/(sqrt(sigma_f * sqrt(pi)));   				 for f = 1:1:NPoints  ;		 wavelet(index, f) = Agauss(f0,sigma_f, A, f);		 end	  	index = index+1;%	f0   end  if plotyon == 'y'	figure	for j = 1 : f0     			plot(wavelet(j, :));		 hold on	endhold off	end