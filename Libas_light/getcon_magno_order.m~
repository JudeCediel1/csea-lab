% getcon_ssvepfaces% searches condnumberes in *dat file generarted by ssvepfaces filefunction [content_cond] = getcon_magno_order(filepath);condvec = []; content_cond = []picvec = [];pleasantvec = [2034 2071 2152  4006:4626 7325 8021:8470];  neutralvec = [2026 2101:2107 2190 2200 2302 2314 2320 2358:2411 2458:2850 5831];unpleasantvec = [2457 3168:3350 6000:6821 9000:9584];fid = fopen(filepath)a = 1;	index = 1 while a > 0	a = fgetl(fid);    if a > 0            index1 = findstr(a, '.j');        index2 = findstr(a, ' ');        if isempty (index1); condvec = [condvec 1];        else condvec = [condvec 2];        end        picnum(index) = str2num(a(index2(length(index2))+1:index2(length(index2))+4))               if index == 1, condvec(length(condvec)) = condvec(length(condvec))+10       elseif ismember(picnum(index), picnum(1:length                        if  ismember(picnum(index), pleasantvec), content_cond = [content_cond  100 + condvec(length(condvec))],         elseif ismember(picnum(index), neutralvec), content_cond = [content_cond  200 + condvec(length(condvec))],         else content_cond = [content_cond  300 + condvec(length(condvec))],          end                                 end    index = index + 1;  endfclose('all')conditionvec_2 = condvec'; content_cond = content_cond'; eval(['save ' filepath '.con content_cond -ascii'])