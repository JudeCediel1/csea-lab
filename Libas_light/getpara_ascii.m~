% avgwinmats% averages SSR-winmats function [paravec] = getpara_ascii(filemat, indexvecrows,indexveccolumns, outname);% subjectvec = ['01'; '02'; '04'; '07'; '08'; '09';'10'; '11'; '12';'13';'14';'15';'17'; '18';'19'; '21'; '22'; '24';'25'];%subjectvec = ['201'; '203'];for subject = 1 : size(filemat,1)		s = findstr('.', [filemat(subject,:)]);		matname = [filemat(subject,1:s(1)-1)];		eval(['load ' filemat(subject,:) ' -ascii']); 		eval(['tmpval = ' matname ';']);						paravec(subject) = mean(tmpval(indexvecrows, indexveccolumns)); 		fprintf('.')		endparavec = paravec'; if ~isempty(outname)disp('saving to disk:')disp(outname)save(outname,  'avgmat' ,  '-ascii')end	