% avgwinmats% averages SSR-winmats function [outmat] = getTavgbaby(filemat);for subject = 1 : size(filemat,1)		    outmat = zeros(size(tmpval,1),5);         s = findstr('.', [filemat(subject,:)]);		matname = [filemat(subject,1:s(1)-1)];		eval(['load ' filemat(subject,:) ' -ascii']); 		eval(['tmpval = X' matname ';']);		tpoints = size(tmpval,2);        fprintf('number of timepoints = %g', tpoints)    fprintf('\r')        divid = floor(tpoints/5);        dividvec = (1:5) * divid;          restveclength = tpoints - divid * 5;                         for index = 1:5        outmat(:,index) = mean(tmpval(:,dividvec(index)-divid+1:dividvec(index),2);         end	        elseif restveclength == 1        for index = 1:5        outmat(:,index) = mean(tmpval(:,dividvec(index)-divid+1:dividvec(index),2);         end	        save([deblank(filemat(subject,:)) '.5t'],  'outmat' ,  '-ascii')end	