% getcon_ssvepfaces% searches condnumberes in *dat file generarted by ssvepfaces filefunction [conditionvec_2] = getcon_magno(filepath);condvec = []; content_cond = []picvec = [];pleasantvec = [2034 2071 2152 2314 4006:5831 7325 8021:8470];  neutralvec = [2026 2101:2107 2190 2200 2302 2320 2358:2411 2458:2850];unpleasantvec = [2457 3168:3350 6000:6821 9000:9584];fid = fopen(filepath)a = 1	 while a > 0	a = fgetl(fid); 	index1 = findstr(a, '.j') 		    if isempty (index1); condvec = [condvec 1];    else condvec = [condvec 2];    end          endfclose('all')conditionvec_2 = condvec'; con4EEG = contentvec1234; eval(['save ' filepath '.con con4EEG -ascii'])