% GENERDATNEWfunction[data]=gd_new();t = [0:0.004:3]; sig1 = sin(2*4*pi*t).*3 + (rand(size(t)).*3);s1 = (rand(1,490)-0.5).*5;s2 = (rand(1,345)-0.5).*5 ;sigAdd = [zeros(1,300) sin(4*pi*t(1:90)).*7 zeros(1,100) ];data1 = [ s1+sigAdd sig1 s2];%data2 =[s2 sig2 s1];%data = data1 + data2;data = data1;%datanoise = rand(1, 2048).*5;taxis = 0plot(data, 'k')%plot(sig1)