function [convector] = get_log_lisafoveal(infilepath);% tastencode 1 = ALTGR = wort% tastencode 2 = ALTGR = pseudowortconvector = []; fid = fopen(infilepath);% dummylinesdumline = fgetl(fid)dumline = fgetl(fid)dumline = fgetl(fid)dumline = fgetl(fid)dumline = fgetl(fid)%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%  line = 1index = 1;			while line > 0				% read data				line = fgets(fid);				if isempty((line(1))), disp(['lines found: ']), disp(index-1), break, end		if line < 0, break, return, end				% segmentiere jede zeile nach tabs				trialnumseg = find(line == '	');				% event-tags							eventcateg = line(trialnumseg(2)+1:trialnumseg(3)-1)        code = line(trialnumseg(3)+1:trialnumseg(4)-1)               if length(code) >3;          		if strcmp (eventcateg(1), 'P') && strcmp(code(1:4), 'erot'), convector = [convector 2];                        elseif strcmp (eventcateg(1), 'P') && strcmp(code(1:4), 'roma'), convector = [convector 4];                           elseif strcmp (eventcateg(1), 'P') && strcmp(code(1:4), 'face'), convector = [convector 6];                            elseif strcmp (eventcateg(1), 'P') && strcmp(code(1:4), 'peop'), convector = [convector 8];                          elseif strcmp (eventcateg(1), 'P') && strcmp(code(1:4), 'thrt'), convector = [convector 10];                        elseif strcmp (eventcateg(1), 'P') && strcmp(code(1:4), 'muti'), convector = [convector 12];                         elseif strcmp (eventcateg(1), 'S') && strcmp(code(1), 'I'), convector = [convector 99];                        elseif strcmp (eventcateg(1), 'S') && strcmp(code(1), 's'), convector = [convector 0];                       elseif strcmp (eventcateg(1), 'S') && strcmp(code(1), 'n'), convector = [convector convector(length(convector))+100];                  end                       end            				index = index + 1;			% for correctflag	end		fclose('all')